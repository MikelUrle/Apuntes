package paquete.modelo.dto;

public class Proyecto {

	private int id;
	private String texto;
	private String nombre;
	
	public Proyecto(int id, String texto, String nombre) {
		super();
		this.id = id;
		this.texto = texto;
		this.nombre = nombre;
	}

	public Proyecto() {
		super();
		this.id = 0;
		this.texto = "";
		this.nombre = "";
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getTexto() {
		return texto;
	}

	public void setTexto(String texto) {
		this.texto = texto;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	@Override
	public String toString() {
		return "Proyecto [id=" + id + ", texto=" + texto + ", nombre=" + nombre + "]";
	}
	
}